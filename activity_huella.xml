<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".huella">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="72dp"
        android:layout_height="68dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="287dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/group_43" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="108dp"
        android:layout_height="39dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="628dp"
        android:text="To Do..."
        android:textAlignment="center"
        android:textColor="#FF8A00"
        android:textSize="22dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="347dp"
        android:layout_height="102dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="132dp"
        android:text="Registra tu entrada o salida a tu empresa"
        android:textAlignment="center"
        android:textColor="#05003C"
        android:textSize="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ProgressBar
        android:id="@+id/progressBar"
        style="?android:attr/progressBarStyle"
        android:layout_width="71dp"
        android:layout_height="78dp"
        android:layout_marginStart="168dp"
        android:layout_marginTop="492dp"
        android:progressTint="#05003C"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="139dp"
        android:layout_height="154dp"
        android:layout_marginStart="136dp"
        android:layout_marginTop="264dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/jjsjs" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="86dp"
        android:layout_height="29dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="444dp"
        android:layout_marginEnd="161dp"
        android:textColor="#05003C"
        android:text="escaneando.."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>